    "@types/node": "^24.1.0",
    "cross-env": "^10.0.0",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.5",
    "typeface-cormorant-garamond": "^1.1.13",
    "typeface-montserrat": "^1.1.13",
